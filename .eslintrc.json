{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [{
			"files": "src/**/*.js"
		}
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
			"quotes": ["error", "double"],
			"curly": "error",
			"eqeqeq": "error",
			"no-alert": "error",
			"semi": "error", 
			"react/prop-types": "off",
			"no-unused-vars": "warn"
    }		
}
